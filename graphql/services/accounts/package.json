{
  "name": "graphql-accounts",
  "description": "The accounts graphql microservice for create-graphql-app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r dotenv/config ./dist/index.js",
    "dev": "PORT=3012 ./startDev.sh",
    "test": "jest",
    "build": "NODE_ENV=production tsc",
    "deploy": "yarn build && gcloud app deploy --project=create-graphql-app"
  },
  "dependencies": {
    "@apollo/federation": "latest",
    "apollo-server": "latest",
    "dotenv": "^8.0.0",
    "graphql": "latest"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/zen-observable": "^0.8.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
